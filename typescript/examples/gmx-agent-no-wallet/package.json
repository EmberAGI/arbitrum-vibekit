{
  "name": "gmx-agent-no-wallet",
  "version": "1.0.0",
  "description": "Ember SDK GMX Agent Example",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --build",
    "dev": "pnpm build && tsx src/index.ts",
    "start": "node dist/index.js",
    "inspect:npx": "pnpm run build && npx -y @modelcontextprotocol/inspector node ./dist/index.js",
    "docker:build": "docker build -t gmx-agent ../../",
    "docker:run": "docker run --env-file .env -p 3001:3001 gmx-agent",
    "docker:compose:up": "docker compose up --build",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\""
  },
  "dependencies": {
    "a2a-samples-js": "workspace:*",
    "@emberai/sdk-typescript": "0.3.0-beta.6",
    "@gmx-io/sdk": "^0.2.0",
    "@modelcontextprotocol/sdk": "^1.5.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2",
    "express": "^4.21.2",
    "tsx": "^4.6.2",
    "viem": "^2.26.3",
    "zod": "^3.22.4",
    "ember-mcp-tool-server": "workspace:*",
    "@openrouter/ai-sdk-provider": "^0.4.5",
    "ai": "^4.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.5.3"
  }
} 