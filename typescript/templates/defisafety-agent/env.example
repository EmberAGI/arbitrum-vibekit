# OpenRouter API key for LLM orchestration
OPENROUTER_API_KEY=your-openrouter-api-key

# Optional: OpenAI API key for embeddings (if not set, doc-rag-mcp-server will use basic search)
OPENAI_API_KEY=your-openai-api-key

# Agent configuration
AGENT_NAME=DeFi Safety Agent
AGENT_VERSION=1.0.0
AGENT_DESCRIPTION=AI agent for scoring DeFi protocols based on DeFiSafety criteria
PORT=3008

# LLM Model selection
LLM_MODEL=google/gemini-2.0-flash-001

# MCP Server configuration
DOC_RAG_MCP_SERVER_URL=stdio://typescript/lib/mcp-tools/doc-rag-mcp-server/dist/index.js

# Optional: Enable CORS
ENABLE_CORS=true 