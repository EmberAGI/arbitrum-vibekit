        "nanoid": "^5.0.7",
        "viem": "^2.26.3",
        "zod": "catalog:"
    },
    "devDependencies": {
        "@types/chai": "catalog:",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/mocha": "catalog:",
        "@types/node": "catalog:",
        "@types/sinon": "^17.0.4",
        "chai": "catalog:",
        "mocha": "catalog:",
        "mocha-suppress-logs": "catalog:",
