# Agent Card URL - The URL of the agent card endpoint
# This should point to your agent card server
NEXT_PUBLIC_AGENT_CARD_URL=http://localhost:3001

# WalletConnect Project ID (required for RainbowKit)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# Enable debug mode for additional logging and UI elements (connections, MCP resources, debug console)
NEXT_PUBLIC_DEBUG_MODE=false

# Privy App ID for embedded wallet support features
NEXT_PUBLIC_PRIVY_APP_ID=your_privy_app_id_here

# Privy API credentials for server-side wallet operations (Basic auth)
PRIVY_APP_SECRET=your_privy_app_secret_here

# Private key for the funding wallet (used to sponsor initial wallet upgrades)
FUNDING_WALLET_PRIVATE_KEY=0x_your_private_key_here

# Executor address for 7702 authorization signing
NEXT_PUBLIC_EXECUTOR_ADDRESS=0x_your_executor_address_here

## Feature flags monitored through environment variables
# Enable or disable the funding feature
NEXT_PUBLIC_FEATURE_FLAG_ENABLE_FUNDING=false
