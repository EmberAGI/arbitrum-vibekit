{
  "name": "langgraph-js-starter",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:delegations-bypass": "DELEGATIONS_BYPASS=true NEXT_PUBLIC_DELEGATIONS_BYPASS=true turbo run dev",
    "qa": "bash scripts/qa.sh",
    "build": "turbo run build",
    "qa:web": "pnpm build && pnpm start",
    "start": "turbo run start",
    "start:delegations-bypass": "DELEGATIONS_BYPASS=true NEXT_PUBLIC_DELEGATIONS_BYPASS=true turbo run start",
    "lint": "turbo run lint",
    "test": "pnpm -r --filter \"./apps/*\" run test",
    "test:unit": "pnpm -r --filter \"./apps/*\" run test:unit",
    "test:int": "pnpm -r --filter \"./apps/*\" run test:int",
    "test:e2e": "pnpm -r --filter \"./apps/*\" run test:e2e",
    "test:watch": "pnpm -r --parallel --filter \"./apps/*\" run test:watch",
    "test:ci": "pnpm -r --filter \"./apps/*\" run test:ci",
    "reset:dev-stack": "bash scripts/reset-dev-stack.sh"
  },
  "devDependencies": {
    "@langchain/langgraph-cli": "^1.0.4",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.8",
    "next": "16.0.8",
    "turbo": "^2.3.3"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "@langchain/core": "^1.0.1",
    "@langchain/langgraph": "1.0.2"
  },
  "pnpm": {
    "patchedDependencies": {
      "@ag-ui/client@0.0.42": "patches/@ag-ui__client@0.0.42.patch",
      "@ag-ui/langgraph@0.0.20": "patches/@ag-ui__langgraph@0.0.20.patch",
      "@copilotkit/react-core@1.50.1": "patches/@copilotkit__react-core@1.50.1.patch",
      "@copilotkit/runtime@1.50.1": "patches/@copilotkit__runtime@1.50.1.patch",
      "@copilotkitnext/core@0.0.33": "patches/@copilotkitnext__core@0.0.33.patch",
      "@copilotkitnext/runtime@0.0.33": "patches/@copilotkitnext__runtime@0.0.33.patch"
    }
  }
}
