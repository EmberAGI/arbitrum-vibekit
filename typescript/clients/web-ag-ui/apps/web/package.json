{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint \"src\" --ext .ts,.tsx",
    "lint:fix": "eslint \"src\" --ext .ts,.tsx --fix",
    "format": "prettier \"{src,tests}/**/*.{ts,tsx,js,jsx,json,md}\" --write",
    "format:check": "prettier \"{src,tests}/**/*.{ts,tsx,js,jsx,json,md}\" --check",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:unit": "vitest run --config vitest.config.unit.ts",
    "test:e2e": "bash -lc 'ENV_FILE=.env.test; [ -f \"$ENV_FILE\" ] || ENV_FILE=.env.test.example; exec node --env-file=\"$ENV_FILE\" ./node_modules/vitest/vitest.mjs run --config vitest.config.e2e.ts --no-file-parallelism --maxConcurrency=1 \"$@\"' --",
    "test:watch": "vitest watch --config vitest.config.unit.ts",
    "test:ci": "pnpm test:unit && pnpm test:e2e"
  },
  "dependencies": {
    "@ag-ui/client": "^0.0.43",
    "@ag-ui/encoder": "^0.0.43",
    "@copilotkit/react-core": "1.51.2",
    "@copilotkit/react-ui": "1.51.2",
    "@copilotkit/runtime": "1.51.2",
    "@copilotkit/runtime-client-gql": "1.51.2",
    "@copilotkit/shared": "^1.51.2",
    "@langchain/langgraph-sdk": "^1.5.5",
    "@metamask/delegation-toolkit": "^0.13.0",
    "@privy-io/react-auth": "^3.9.0",
    "@tanstack/react-query": "^5.90.12",
    "lucide-react": "^0.561.0",
    "next": "16.0.7",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "rxjs": "^7.8.1",
    "uuid": "^13.0.0",
    "viem": "^2.26.3",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "eslint-plugin-next": "^0.0.0",
    "jsdom": "^27.4.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.8"
  }
}
