{
  "name": "agent-pendle",
  "version": "0.0.1",
  "description": "",
  "main": "dist/agent.js",
  "type": "module",
  "scripts": {
    "dev": "npx @langchain/langgraph-cli dev --port 8125 --no-browser",
    "start": "npx @langchain/langgraph-cli dev --port 8125 --no-browser",
    "build": "tsc --project tsconfig.json",
    "ci:build": "pnpm typecheck && pnpm build",
    "test": "pnpm test:unit && pnpm test:int && pnpm test:e2e",
    "test:unit": "vitest run --config vitest.config.unit.ts",
    "test:int": "bash -lc 'ENV_FILE=.env.test; [ -f \"$ENV_FILE\" ] || ENV_FILE=.env.test.example; exec tsx --env-file=\"$ENV_FILE\" ./node_modules/vitest/vitest.mjs run --config vitest.config.int.ts \"$@\"' --",
    "test:e2e": "bash -lc 'ENV_FILE=.env.test; [ -f \"$ENV_FILE\" ] || ENV_FILE=.env.test.example; exec tsx --env-file=\"$ENV_FILE\" ./node_modules/vitest/vitest.mjs run --config vitest.config.e2e.ts --no-file-parallelism --maxConcurrency=1 \"$@\"' --",
    "test:watch": "vitest watch",
    "test:ci": "pnpm test:unit && pnpm test:int",
    "lint": "eslint \"src\" --ext .ts",
    "lint:fix": "eslint \"src\" --ext .ts --fix",
    "typecheck": "tsc --noEmit --project tsconfig.eslint.json",
    "format": "prettier \"src/**/*.{ts,tsx,js,jsx}\" --write",
    "format:check": "prettier \"src/**/*.{ts,tsx,js,jsx}\" --check"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/html-to-text": "^9.0.4",
    "@types/node": "^22.19.2",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitest/coverage-v8": "^4.0.8",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "2.32.0",
    "prettier": "catalog:",
    "tsx": "catalog:",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "@copilotkit/sdk-js": "1.50.1",
    "@copilotkit/shared": "1.50.1",
    "@emberai/agent-node": "^1.0.0",
    "@langchain/core": "^1.0.1",
    "@langchain/langgraph": "1.0.2",
    "@langchain/langgraph-api": "1.1.11",
    "@langchain/langgraph-checkpoint": "1.0.0",
    "@langchain/openai": "^1.1.3",
    "@metamask/delegation-toolkit": "^0.13.0",
    "node-cron": "^4.2.1",
    "permissionless": "^0.2.57",
    "uuid": "^13.0.0",
    "viem": "catalog:",
    "zod": "^4.3.5"
  }
}
