{
  "name": "agent-gmx",
  "version": "0.0.1",
  "description": "",
  "main": "agent-gmx/graph.ts",
  "type": "module",
  "scripts": {
    "dev": "npx @langchain/langgraph-cli dev --port 8124 --no-browser",
    "build": "tsc --project tsconfig.json",
    "clean": "rm -rf dist",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=\\.test\\.ts$ --testPathIgnorePatterns=\\.int\\.test\\.ts$",
    "test:int": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=\\.int\\.test\\.ts$",
    "format": "prettier --write .",
    "lint": "eslint src",
    "format:check": "prettier --check .",
    "lint:langgraph-json": "node scripts/checkLanggraphPaths.js",
    "lint:all": "yarn lint & yarn lint:langgraph-json & yarn format:check",
    "test:all": "yarn test && yarn test:int && yarn lint:langgraph"
  },
  "devDependencies": {
    "@types/html-to-text": "^9.0.4",
    "@types/node": "^22.19.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitest/coverage-v8": "^4.0.8",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "2.32.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "@copilotkit/sdk-js": "1.10.6",
    "@copilotkit/shared": "^1.10.6",
    "@emberai/agent-node": "^1.0.0",
    "@langchain/core": "^1.0.1",
    "@langchain/langgraph": "1.0.2",
    "@langchain/langgraph-checkpoint": "1.0.0",
    "@langchain/openai": "^1.1.3",
    "@metamask/delegation-toolkit": "^0.13.0",
    "node-cron": "^4.2.1",
    "permissionless": "^0.2.57",
    "uuid": "^13.0.0",
    "viem": "^2.26.3",
    "zod": "^4.1.13"
  }
