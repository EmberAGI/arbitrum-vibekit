{
  "name": "agent-polymarket",
  "version": "0.0.1",
  "description": "Polymarket arbitrage agent for prediction markets",
  "main": "dist/agent.js",
  "type": "module",
  "scripts": {
    "dev": "npx @langchain/langgraph-cli dev --port 8125 --no-browser",
    "build": "tsc --project tsconfig.json",
    "ci:build": "pnpm typecheck && pnpm build",
    "test": "pnpm test:unit",
    "test:unit": "vitest run --config vitest.config.unit.ts",
    "test:watch": "vitest watch",
    "lint": "eslint \"src\" \"tests\" --ext .ts",
    "lint:fix": "eslint \"src\" \"tests\" --ext .ts --fix",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "format": "prettier \"{src,tests}/**/*.{ts,tsx,js,jsx}\" --write",
    "format:check": "prettier \"{src,tests}/**/*.{ts,tsx,js,jsx}\" --check"
  },
  "keywords": ["polymarket", "arbitrage", "prediction-markets", "langgraph"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.19.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitest/coverage-v8": "^4.0.8",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "@copilotkit/sdk-js": "1.10.6",
    "@copilotkit/shared": "^1.10.6",
    "@ethersproject/wallet": "^5.7.0",
    "@langchain/core": "^1.0.1",
    "@langchain/langgraph": "1.0.2",
    "@langchain/langgraph-checkpoint": "1.0.0",
    "@langchain/openai": "^1.1.3",
    "@polymarket/clob-client": "^4.22.8",
    "uuid": "^13.0.0",
    "viem": "^2.26.3",
    "zod": "^3.24.0"
  }
}
