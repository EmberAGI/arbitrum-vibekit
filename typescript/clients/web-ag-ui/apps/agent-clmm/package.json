{
  "name": "agent-clmm",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "npx @langchain/langgraph-cli dev --port 8123 --no-browser",
    "build": "tsc --project tsconfig.json",
    "test": "pnpm test:unit && pnpm test:int && pnpm test:e2e",
    "test:unit": "vitest run --config vitest.config.unit.ts",
    "test:int": "tsx --env-file=.env.test ./node_modules/vitest/vitest.mjs run --config vitest.config.int.ts",
    "test:e2e": "tsx --env-file=.env.test ./node_modules/vitest/vitest.mjs run --config vitest.config.e2e.ts --no-file-parallelism --maxConcurrency=1",
    "test:watch": "vitest watch",
    "test:ci": "pnpm test:unit && pnpm test:int",
    "lint": "eslint \"src\" \"tests\" --ext .ts",
    "lint:fix": "eslint \"src\" \"tests\" --ext .ts --fix",
    "format": "prettier \"{src,tests}/**/*.{ts,tsx,js,jsx}\" --write",
    "format:check": "prettier \"{src,tests}/**/*.{ts,tsx,js,jsx}\" --check",
    "workflow:demo": "tsx --env-file=.env ./scripts/run-camelot-demo.ts",
    "wallet:withdraw": "tsx --env-file=.env scripts/manualWithdraw.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/html-to-text": "^9.0.4",
    "@types/node": "^22.19.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitest/coverage-v8": "^4.0.8",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "2.32.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "@copilotkit/sdk-js": "1.10.6",
    "@copilotkit/shared": "^1.10.6",
    "@emberai/agent-node": "^1.0.0",
    "@langchain/core": "^1.0.1",
    "@langchain/langgraph": "1.0.2",
    "@langchain/langgraph-checkpoint": "1.0.0",
    "@langchain/openai": "^1.1.3",
    "@metamask/delegation-toolkit": "^0.13.0",
    "node-cron": "^4.2.1",
    "permissionless": "^0.2.57",
    "uuid": "^13.0.0",
    "viem": "^2.26.3",
    "zod": "^4.1.13"
  }
}
