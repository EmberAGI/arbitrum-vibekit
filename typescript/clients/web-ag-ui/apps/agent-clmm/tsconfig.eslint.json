{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "noEmit": true,
    "rootDir": ".",
    "paths": {
      "@copilotkit/sdk-js/langgraph": ["src/types/copilotkit-langgraph.d.ts"]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "tests/**/*.ts",
    "vitest.config.*.ts"
  ],
  "exclude": [
    "dist",
    "coverage",
    "node_modules"
  ]
}
